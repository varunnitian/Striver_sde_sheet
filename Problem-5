#include <bits/stdc++.h>

pair<int,int> missingAndRepeating(vector<int> &arr, int n)
{
	int rep=-1;
	int mis=-1;
	sort(arr.begin(),arr.end());
	/*finding the missing number*/
	int total=0,x=1,sum=0;
	for(int i=0;i<n;i++)
	{
		total+=x;
		x++;
		sum+=arr[i];
	}
	mis=(total+rep)-sum;
	/*finding the repeating number*/
	for(int i=1;i<n;i++)
	{
		if(arr[i-1] == arr[i])
		{
			rep=arr[i];
		}
		else
		{
			continue;
		}
	}
	return {mis,rep};
	
}
/*t.C-> O(n) S.C->O(1)*/
